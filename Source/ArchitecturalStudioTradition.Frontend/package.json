{
  "name": "archtradition",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:selfhost",
    "build": "npm run build:selfhost",
    "start:selfhost": "SET NODE_ENV=local && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng run archtradition:serve-selfhost",
    "build:selfhost": "npm run build:selfhost:ng && npm run build:selfhost:copyfiles",
    "build:selfhost:ng": "ng run archtradition:build-selfhost:production",
    "build:selfhost:copyfiles": "cpy robots.txt *.html akamai/** ../../dist/selfhost --cwd=src/static --parents",
    "start:ssr": "ng run archtradition:serve-ssr",
    "serve:ssr": " node dist/server/main",
    "serve:ssr:local": "SET ARCHTRADITION__FRONTEND__SsrWebApiUrl=http://arch-tradition.com/ && SET ARCHTRADITION__FRONTEND__BaseUrl=http://localhost:4000 && SET ARCHTRADITION__FRONTEND__UseProdRobots=true && npm run serve:ssr",
    "build:ssr": "npm run build:ssr:browser && npm run build:ssr:server",
    "build:ssr:browser": "ng run archtradition:build-ssr:production",
    "build:ssr:server": "npm run build:ssr:server:universal && npm run build:ssr:server:copyfiles",
    "build:ssr:server:universal": "ng run archtradition:server:production",
    "build:ssr:server:copyfiles": "cpy *.txt *.html akamai/** ../../dist/server/static --cwd=src/static --parents",
    "pre-build": "npm run test",
    "test:local": "SET NODE_ENV=local && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test archtradition",
    "test:local:chrome": "SET BROWSER=Chrome && npm run test:local",
    "test": "ng test archtradition --watch=false --source-map=false",
    "test-with-coverage-lcov": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test archtradition --watch=false --code-coverage --reporters=teamcity,coverage-istanbul",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "1.2.5",
    "@angular/animations": "~14.1.0",
    "@angular/cdk": "~14.1.0",
    "@angular/common": "~14.1.0",
    "@angular/compiler": "~14.1.0",
    "@angular/core": "~14.1.0",
    "@angular/forms": "~14.1.0",
    "@angular/platform-browser": "~14.1.0",
    "@angular/platform-browser-dynamic": "~14.1.0",
    "@angular/platform-server": "~14.1.0",
    "@angular/router": "~14.1.0",
    "@angular/service-worker": "14.1.0",
    "@auth0/angular-jwt": "5.1.0",
    "@ngneat/transloco": "^4.1.1",
    "@ngrx/store": "14.0.2",
    "@ngrx/store-devtools": "^14.0.2",
    "@nguniversal/common": "^14.0.3",
    "@nguniversal/express-engine": "^14.0.3",
    "angular-connection-service": "13.0.1",
    "bootstrap-sass": "^3.4.3",
    "gsap": "3.10.4",
    "http-server": "14.1.1",
    "ionicons": "6.0.3",
    "moment": "2.29.4",
    "moment-duration-format": "2.3.2",
    "ng-mocks": "14.1.1",
    "ngx-build-plus": "^14.0.0",
    "ngx-captcha": "12.0.2",
    "ngx-infinite-scroll": "14.0.0",
    "normalize.css": "8.0.1",
    "proj4": "2.8.0",
    "redux": "4.2.0",
    "rxjs": "7.5.6",
    "smooth-scrollbar": "8.7.5",
    "swiper": "^8.4.3",
    "tslib": "2.4.0",
    "zone.js": "0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1401.0",
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "14.1.0",
    "@angular/compiler-cli": "14.1.0",
    "@angular/language-service": "14.1.0",
    "@types/core-js": "2.5.5",
    "@types/google.analytics": "0.0.35",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^1.10.33",
    "@types/moment-duration-format": "2.2.2",
    "@types/node": "^12.11.1",
    "blob-polyfill": "^5.0.20210201",
    "bundle-scss": "1.5.1",
    "core-js": "3.24.1",
    "cpy-cli": "4.1.0",
    "d3": "7.6.1",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "0.0.32",
    "karma-teamcity-reporter": "^1.1.0",
    "ng-packagr": "14.2.1",
    "protractor": "7.0.0",
    "ts-node": "10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  }
}
