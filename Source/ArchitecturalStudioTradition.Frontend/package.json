{
  "name": "arch-tradition",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all clear --parallel start:**",
    "build": "npm run build:selfhost",
    "start:selfhost": "SET NODE_ENV=local && node ./scripts/wait-for-lib.js && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng run archtradition:serve-selfhost",
    "start:lib": "ng build archtradition-contract --watch",
    "build:selfhost": "npm run build:selfhost:ng && npm run build:selfhost:copyfiles",
    "build:selfhost:ng": "ng run archtradition:build-selfhost:production",
    "build:selfhost:copyfiles": "cpy robots.txt *.html akamai/** ../../dist/selfhost --cwd=src/static --parents",
    "start:ssr": "ng run archtradition:serve-ssr",
    "serve:ssr": " node dist/server/main",
    "serve:ssr:local": "SET ARCHTRADITION__FRONTEND__SsrWebApiUrl=http://arch-tradition.com/ && SET ARCHTRADITION__FRONTEND__BaseUrl=http://localhost:4000 && SET ARCHTRADITION__FRONTEND__UseProdRobots=true && npm run serve:ssr",
    "build:ssr": "npm run build:ssr:browser && npm run build:ssr:server",
    "build:ssr:browser": "ng run archtradition:build-ssr:production",
    "build:ssr:server": "npm run build:ssr:server:universal && npm run build:ssr:server:copyfiles",
    "build:ssr:server:universal": "ng run archtradition:server:production",
    "build:ssr:server:copyfiles": "cpy *.txt *.html akamai/** ../../dist/server/static --cwd=src/static --parents",
    "pre-build": "npm run test",
    "build:contract": "ng build archtradition-contract",
    "publish:contract": "npm run build:contract && cd dist/archtradition-contract && npm pack && npm publish --registry http://localhost:4873",
    "test:local": "SET NODE_ENV=local && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test archtradition",
    "test:local:chrome": "SET BROWSER=Chrome && npm run test:local",
    "test": "ng test archtradition --watch=false --source-map=false",
    "test-with-coverage-lcov": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test archtradition --watch=false --code-coverage --reporters=teamcity,coverage-istanbul",
    "archtradition-contract:patch": "cd ./projects/archtradition-contract && npm version patch && cd ../../",
    "archtradition-contract:scssbundle": "scss-bundle --rootDir ./projects/styles/archtradition-contract -e ./projects/archtradition-contract/styles/theme.scss -o ./projects/archtradition-contract/theme.scss",
    "archtradition-contract:cssindlude": "node-sass ./projects/archtradition-contract/theme.scss ./dist/archtradition-contract/theme.css",
    "archtradition-contract:build": "ng build archtradition-contract && npm-run-all archtradition-contract:scssbundle archtradition-contract:cssindlude",
    "archtradition-contract:publish": "npm-run-all archtradition-contract:patch archtradition-contract:build && cd dist/archtradition-contract && npm pack && npm publish --registry http://localhost:4873",
    "clear": "node ./scripts/clear.js",
    "watch": "ng build --watch --configuration development",
    "prerender": "ng run archtradition:prerender"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^1.2.5",
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/platform-server": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@angular/service-worker": "^15.0.1",
    "@ngrx/effects": "^15.0.0",
    "@ngrx/store": "^15.0.0",
    "@ngrx/store-devtools": "^15.0.0",
    "@nguniversal/express-engine": "^15.0.0",
    "@types/google.analytics": "^0.0.42",
    "@types/moment-duration-format": "^2.2.3",
    "angular-connection-service": "^13.0.1",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "express": "^4.15.2",
    "gsap": "^3.11.3",
    "http-server": "^14.1.1",
    "moment-duration-format": "^2.3.2",
    "rxjs": "~7.5.0",
    "smooth-scrollbar": "^8.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "15.0.0-beta.0",
    "@angular-devkit/build-angular": "^15.0.1",
    "@angular-eslint/builder": "15.1.0",
    "@angular-eslint/eslint-plugin": "15.1.0",
    "@angular-eslint/eslint-plugin-template": "15.1.0",
    "@angular-eslint/schematics": "15.1.0",
    "@angular-eslint/template-parser": "15.1.0",
    "@angular/cli": "~15.0.1",
    "@angular/compiler-cli": "^15.0.0",
    "@ngrx/eslint-plugin": "^15.0.0",
    "@nguniversal/builders": "^11.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "^8.28.0",
    "guess-parser": "^0.4.22",
    "guess-webpack": "^0.4.22",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.0.1",
    "typescript": "~4.8.2"
  }
}